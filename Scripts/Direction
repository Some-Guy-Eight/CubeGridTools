namespace SlimeGame {
    public enum Direction {
        Right,
        Left,
        Up,
        Down,
        Fwd,
        Back,

        RightUp,
        RightDown,
        RightFwd,
        RightBack,
        LeftUp,
        LeftDown,
        LeftFwd,
        LeftBack,
        UpFwd,
        UpBack,
        DownFwd,
        DownBack,

        RightUpFwd,
        RightUpBack,
        RightDownFwd,
        RightDownBack,
        LeftUpFwd,
        LeftUpBack,
        LeftDownFwd,
        LeftDownBack,

        None,

        Default = Fwd,
    }
